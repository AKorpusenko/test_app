<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <h2>Sign up</h2>
      </br>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>

        <div class="field">
          <%= f.label :email, :class => "control-label" %><br />
          <%= f.email_field :email, autofocus: true, class: :"form-control", id: :"inputSuccess1" %>
        </div>

        <div class="field">
          <%= f.label :password, :class => "control-label" %>
          <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
          <%= f.password_field :password, autocomplete: "off", class: :"form-control", id: :"inputSuccess1" %>
        </div>

        <div class="field">
          <%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation, autocomplete: "off", class: :"form-control", id: :"inputSuccess1" %>
        </div>
        </br>
        <div class="actions">
          <%= f.submit "Sign up", class: :'btn btn-primary btn-block' %>
        </div>
      <% end %>
      </br>
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>